.right__nod
	right: 0
.selector__nod
	.attribute_list
		.selector
			background: none
			border: 1px solid rgb(189, 194, 201)
			height: 40px
			line-height: 40px
			span
				background: none
				line-height: 40px
				&:after
					content: "\f107"
					display: inline-block
					font: normal normal normal 14px/1 FontAwesome
					font-size: 20px
					position: absolute
					right: 10px
					text-rendering: auto
					top: 10px
					transform: translate(0, 0)
			.form-control
				height: 40px !important
				width: 100% !important
				max-width: 100% !important
				right: 0
				option
					line-height: 40px
