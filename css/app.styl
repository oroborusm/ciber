@media screen and (max-width: 990px)
	.right-block
		padding 0
.user__info
	//height: 55px
	padding: 5px 10px
	vertical-align: middle
	width: calc(100% - 60px)
	.header_user_info
		border: none
		float: none
		.logged
			&:hover
				.logout
					opacity: 1
					//pointer-events: initial
					top: 40px
					visibility: visible
		a
			color: #666
			font-family: 'Oswald', sans-serif
			font-size: 16px
			padding: 0
			span
				color: #666
				font-family: 'Oswald', sans-serif
				text-transform: uppercase
			&:hover
				background: transparent
		.login
			font-family: 'Oswald', sans-serif
			font-weight: 100
			text-transform: full-width
		.logout
			opacity: 0
			transition: .25s
			visibility: hidden
.visible
	display: block !important

